    "-- 20. Find the position with the highest average salary.\n",
    "       \n",
    "       SELECT \"EmpPosition\", AVG(\"Salary\") AS AverageSalary \n",
    "       FROM workers \n",
    "       WHERE \"Salary\" IS NOT NULL \n",
    "       GROUP BY \"EmpPosition\" \n",
    "       ORDER BY AverageSalary DESC \n",
    "       LIMIT 1;\n"
   ]

    "--5 record type\n",
    "\n",
    "do\n",
    "$$\n",
    "declare\n",
    "   rec record;\n",
    "begin\n",
    "\n",
    "   select \"Car\", \"Weight\", \"Origin\"\n",
    "   into rec\n",
    "   from cars\n",
    "   where \"Car\" = 'Ford Torino';\n",
    "\n",
    "   raise notice 'name :%, \"Weight :%, Origin :%',rec.\"Car\",rec.\"Weight\", rec.\"Origin\";\n",
    "end;\n",
    "$$\n",
    "language plpgsql;"
   ]
